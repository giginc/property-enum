{
    "name": "giginc/property-enum",
    "description": "Property based enum plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "enum"
    ],
    "homepage": "https://github.com/giginc/property-enum",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakedc/enum": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "PropertyEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PropertyEnum\\Test\\": "tests"
        }
    }
}
